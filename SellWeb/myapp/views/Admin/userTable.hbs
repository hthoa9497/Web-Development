<!-- page content -->
<div class="right_col" role="main">
<div class="">
  <div class="page-title">
    <div class="title_left">
        <h3>
            Bảng Thông tin khách hàng
        </h3>
    </div>
  </div>
  <div class="clearfix"></div>

  <div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
      <div class="x_panel">
        <div class="x_title">
          <h2>Khách hàng</h2>
          <div class="clearfix"></div>
        </div>
        <div class="x_content">
          <table id="datatable" class="table table-striped table-bordered">
            <thead>
              <tr>
                <th>Họ tên</th>
                <th>Ngày sinh</th>
                <th>Email</th>
                <th>SĐT</th>
                <th>Username</th>
              </tr>
            </thead>
            <tbody>
              {{# each Users}}
              {{# each this}}
              <tr>
                <td>{{this.name}}</td>
                <td>{{this.date_of_birth}}</td>
                <td>{{this.email}}</td>
                <td>{{this.phone}}</td>
                <td>{{this.username}}</td>
              </tr>
              {{/each}}
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>
      <!-- footer content -->
      <!-- /footer content -->
    </div>
    <!-- /page content -->
  </div>

</div>



 